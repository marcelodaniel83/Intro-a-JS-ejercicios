// Un alumno debe calcular su promedio en una materia en la que tiene 3 notas. Las notas van de 1 a 10

var nota1= 0;
var nota2=0;
var nota3=0;
nota1 = parseFloat(prompt('Ingrese nota 1: '));
nota2 = parseFloat(prompt('Ingrese nota 2: '));
nota3= parseFloat(prompt('Ingrese nota 3: '));
alert('El promedio es: '+ (nota1+nota2+nota3)/3);
